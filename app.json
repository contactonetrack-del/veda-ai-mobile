{
  "expo": {
    "name": "VEDA AI",
    "slug": "veda-ai-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/veda-avatar.png",
    "scheme": "veda-ai",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/veda-avatar.png",
      "resizeMode": "contain",
      "backgroundColor": "#212121"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vedaai.mobile"
    },
    "android": {
      "package": "com.vedaai.mobile",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/veda-avatar.png",
        "backgroundColor": "#212121"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#212121",
          "image": "./assets/veda-avatar.png",
          "dark": {
            "image": "./assets/veda-avatar.png",
            "backgroundColor": "#212121"
          },
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2caec5a8-505f-45a5-855d-646640bec01e"
      }
    }
  }
}